          "_name": "features14",
          "_customHTML": "<section class=\"features14\" group=\"Features\" plugins=\"amp-main\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" value=\"6\" step=\"1\">\n    <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" value=\"7\" step=\"1\">\n    <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n    <input type=\"checkbox\" title=\"Show Line\" name=\"showLine\">\n    \n    <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#bbc3cc\" condition=\"showLine\">\n    <input type=\"checkbox\" title=\"Show Main Text\" name=\"showMainText\">\n    <input type=\"range\" inline title=\"Items Count\" name=\"itemsCount\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n    <input type=\"checkbox\" title=\"Show Icons\" name=\"showIcons\" checked>\n    <input type=\"range\" inline title=\"Icons Size\" name=\"iconsSize\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"4\" condition=\"showIcons\">\n    <input type=\"color\" title=\"Icon Color 1\" name=\"iconColor1\" value=\"#3a5d70\" condition=\"showIcons\">\n    <input type=\"color\" title=\"Icon Color 2\" name=\"iconColor2\" value=\"#3a5d70\" condition=\"showIcons\">\n    <input type=\"color\" title=\"Icon Color 3\" name=\"iconColor3\" value=\"#3a5d70\" condition=\"showIcons\">\n    <input type=\"checkbox\" title=\"Show Card Title\" name=\"showCardTitle\">\n    <input type=\"checkbox\" title=\"Show Card Text\" name=\"showCardText\" checked>\n    <input type=\"checkbox\" title=\"Show Button\" name=\"showButton\">\n    <input type=\"color\" title=\"Card Bg Color\" name=\"cardBgColor\" value=\"#f7f7f7\">\n    <fieldset type=\"background\" name=\"bg\">\n        <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/background1.jpg\">\n        <input type=\"color\" title=\"Background Color\" value=\"#fffff